    <TextField fx:id="ingresonombres" layoutX="175.0" layoutY="51.0"/>
    <TextField fx:id="ingresocorreo" layoutX="175.0" layoutY="95.0" promptText="example@example.com"/>
    <Button fx:id="botonregresarinicio" layoutX="58.0" layoutY="228.0" mnemonicParsing="false"
            text="Regresar al inicio" onAction="#regresarInicio"/>
